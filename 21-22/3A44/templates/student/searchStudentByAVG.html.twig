<h1>Search Student By AVG</h1>
{{ form(searchStudentByAVG) }}
<table border="1">
    <tr>
        <td>NCE</td>
        <td>Username</td>
        <td>Email</td>
        <td>Moyenne</td>
        <td>Classroom</td>
    </tr>

    {% for student in students %}
        <tr>
            <td>{{student.nce}}</td>
            <td>{{student.username}}</td>
            <td>{{student.Email}}</td>
            <td>{{student.classroom}}</td>
            <td>{{student.moyenne}}</td>
        </tr>
    {% endfor %}
</table>
